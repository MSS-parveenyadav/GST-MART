@model GST_BLL.DTO.AdminPathModel
@{
    ViewBag.Title = "GST MART";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
@using (Html.BeginForm("AdminPath", "Admin", FormMethod.Post))
{
    @*@Html.AntiForgeryToken()*@
    
    @section Submenu
{

        @Html.Partial("~/Views/_Partial/_Setting.cshtml")
    }

    @Html.ValidationSummary(true)
    <div class="container-fluid">
        <div class="tab-pane active" id="home">
            <div class="tab-conentmain">
                <div class="tab-pane active" id="home">
                    <div class="tab-conentmain">
                        <div class="data-table">

                            <div class="container-fluid">
                                <div class="w-bg">

                                    <table id="example" class="table table-striped " width="100%" cellspacing="0">

                                        <tbody>
                                            <tr>
                                                <td class="col-md-1" align="right"><label>Admin Path</label></td>
                                                <td class="col-md-2 cmg">
                                                    @Html.HiddenFor(m => m.ID)
                                                    @Html.TextBoxFor(m => m.AdminPath, "", new { Value = "", placeholder = "Admin Path" })<p style="color:green">@TempData["Status"]</p><p style="color:red">@TempData["Error"]</p>
                                                </td>
                                            </tr>
                                        </tbody>
                                    </table>

                                </div>
                                <div class=" button">
                                    <input name="" type="submit" value="Update">
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

    </div>
    <div class="data-table">
        <div class="container-fluid">
            <table id="example" class="table table-striped table-bordered " width="100%" cellspacing="0">
                <thead>
                    <tr>
                        <th>File Location <i class="icone1"> </i></th>
                    </tr>
                </thead>
                <tbody>
                            <tr>
                                <td>@TempData["AdminPath"]</td>
                            </tr>
                </tbody>
            </table>





        </div>
    </div>
}

