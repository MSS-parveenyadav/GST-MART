
@model   IEnumerable<GST_BLL.DTO.LoogerModel>

@{
    ViewBag.Title = "CycleErrors";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}


@if (Model.Count() != 0)
{
    <h2>Ledger Mismatch Errors</h2>

    <div class="data-table">
        <div class="container-fluid">

            <table id="example" class="table table-striped table-bordered " width="100%" cellspacing="0" border="1">


                <thead>
                    <tr>
                        <th>Error Column</th>
                        <th>Error Code </th>
                        <th>ErrorColumn  </th>
                    </tr>
                </thead>

                @foreach (var item in Model)
                {
                    <tr>
                        <td>@item.Flat_File_Source_Error_Output_Column</td>
                        <td>@item.ErrorCode</td>
                        <td>@item.ErrorColumn</td>
                    </tr>
                }
            </table>


        </div>
    </div>
    <br />
    <br />
}

